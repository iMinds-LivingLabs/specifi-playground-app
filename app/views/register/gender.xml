<Alloy>
	<View id="registerGender">
		<View id="registerGenderWrapper">
			<View class="radiosWrapper">
				<View class="innerRadiosWrapper">
					<Require type="view" src="ui/checkbox" id="maleRadio"  title="Male" onChange="maleBoxOutput"/>
					<Require type="view" src="ui/checkbox" id="femaleRadio"  title="Female" onChange="femaleBoxOutput"/>
				</View>
			</View>
			<View class="labelWrapper">
				<Label class="LabelRegister" id="labelTop"></Label>
			</View>
			<Require type="view" src="ui/extendedbutton" class="registerButton" id="BtnNext" onClick="onClick"/>
			<Require type="view" src="ui/extendedbutton" class="registerButton" id="BtnCancel" onClick="onClick"/>
		</View>
	</View>
</Alloy>